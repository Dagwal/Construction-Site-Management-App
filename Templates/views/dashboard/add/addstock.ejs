            <%- include('../partials/header.ejs') %>
            <div class="container-fluid">
            <div class="row">
                  <%- include('../partials/nav.ejs', { currentPageName: 'add-stock' }) %>    
                  <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4 main">
                    <div class="container ">
                    <form action="http://localhost:3000/api/materialinventory" method="post" class="insert-form ">
                        <hr>
                        <h1 class="text-center">Add Material Inventory</h1>
            
                        <hr>
            
                        <div class="input-field">
            
                            <div class="table-responsive">
            
                                <table class="table table-bordered text-center" id="table_field">
            
                                    <tr>
            
                                        
            
                                        <th>Item No.</th>
            
                                        <th>Material Name</th>
        
                                        <th>Material Type/Size</th>
        
                                        <th>Quantity</th>
        
                                        <th>Remove</th>
            
                                    </tr>
            
                                    <tr>
            
                                        
            
                                        <td class="itemnobox"><select class="form-control" name="itemNumber" required>
                                          <% if(typeof materials !== 'undefined') { %>
                                            <% materials.forEach(function(material) { %>
                                              <option class="form-control" value="<%= material.itemNumber %>"><%= material.itemNumber %></option>
                                            <% }); %>
                                         <% } %>
                                        </select></td>
            
                                        <td><select class="form-control" name="materialName" required>
                                          <% if(typeof materials !== 'undefined') { %>
                                            <% materials.forEach(function(material) { %>
                                              <option class="form-control" value="<%= material.materialName %>"><%= material.materialName %></option>
                                            <% }); %>
                                         <% } %>
                                          
                                        </select></td>
        
                                        <td><select class="form-control" name="materialName" required>
                                          <% if(typeof materials !== 'undefined') { %>
                                            <% materials.forEach(function(material) { %>
                                              <option class="form-control" value="<%= material.materialName %>"><%= material.materialTypeSize %></option>
                                            <% }); %>
                                         <% } %>
                                          
                                        </select></td>
        
                                        <td><input type="number" class="form-control" name="quantity" required></td>
        
                                        <td><input class="btn btn-danger" name="remove" id="remove" value="Remove" type="button"></td>
            
                                    </tr>
            
                                </table>
            
                                <div class="row">
            
                                    <div class="col-12 text-right">
            
                                        <input type="button" name="add" id="add" value="Add" class="btn btn-success">
            
                                    </div>
            
                                    <div class="col-12 m-3">
            
                                      <input type="submit" name="save" id="save" value="Save" class="btn btn-primary btn-block">
            
                                    </div>
            
                                </div>
            
                            </div>
            
                        </div>
            
                    </form>
                    <br>
                    <% if (typeof message !== 'undefined') { %>
                      <div  role="alert">
                        <div class="alert alert-warning d-flex align-items-center" role="alert">
                          <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" class="bi bi-exclamation-triangle-fill flex-shrink-0 me-2" viewBox="0 0 16 16" role="img" aria-label="Warning:">
                            <path d="M8.982 1.566a1.13 1.13 0 0 0-1.96 0L.165 13.233c-.457.778.091 1.767.98 1.767h13.713c.889 0 1.438-.99.98-1.767L8.982 1.566zM8 5c.535 0 .954.462.9.995l-.35 3.507a.552.552 0 0 1-1.1 0L7.1 5.995A.905.905 0 0 1 8 5zm.002 6a1 1 0 1 1 0 2 1 1 0 0 1 0-2z"/>
                          </svg>
                          <div>
                            <%= message %>
                          </div>
                        </div>
                      </div>
                    <% } %>
                    <form action="http://localhost:3000/api/materials" method="post" class="insert-form ">
                      <hr>
                      <h1 class="text-center">Add Materials Size or Type</h1>
          
                      <hr>
          
                      <div class="input-field">
          
                          <div class="table-responsive">
          
                              <table class="table table-bordered text-center" id="table_field">
          
                                  <tr>
          
                                      
                                      <th>Item No.</th>
                                      <th>Material Name</th>
      
                                      <th>Material Type/Size</th>
      
                                      <th>Remove</th>
          
                                  </tr>
          
                                  <tr>
          
                                      
                                    <td class="itemnobox"><input type="text" class="form-control" name="itemNumber" required></td>
                                      <td><input type="text" class="form-control" name="materialName" required></td>
          
                                      <td><input type="text" class="form-control" name="materialTypeSize" required></td>
      
                                      <td><input class="btn btn-danger" name="remove" id="remove" value="Remove" type="button"></td>
          
                                  </tr>
          
                              </table>
          
                              <div class="row">
          
                                  <div class="col-12 text-right">
          
                                      <input type="button" name="add" id="add" value="Add" class="btn btn-success">
          
                                  </div>
          
                                  <div class="col-12 m-3">
          
                                    <input type="submit" name="save" id="save" value="Save" class="btn btn-primary btn-block">
          
                                  </div>
          
                              </div>
          
                          </div>
          
                      </div>
          
                  </form>
                </div>
                    </main>
                
            </div>
            </div>
